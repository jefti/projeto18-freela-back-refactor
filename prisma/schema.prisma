generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id   Int    @id @default(autoincrement())
  email Int @unique
  nome String
  foto String
  CPF String
  phone String
  senha String
  pokemons pokemon[]

  @@map("users")
}

model pokemon {
  id   Int    @id @default(autoincrement())
  descricao String
  diaria Int
  especie String
  foto String
  comentarioFoto String
  disponivel Boolean @default(true)
  userId Int
  user user @relation(fields: [userId], references: [id])

  @@map("pokemons")
}